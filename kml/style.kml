	<Style id="footprint">
		<LabelStyle>
			<color>50D2783C</color>
		</LabelStyle>
		<BalloonStyle><bgColor>00000000</bgColor></BalloonStyle>
		<LineStyle>
			<color>99f00fff</color>
			<width>3</width>
		</LineStyle>
		<PolyStyle>
			<color>007f0eff</color>
			<colorMode>random</colorMode>
			<fill>0</fill>
		</PolyStyle>
	</Style>
	<Style id="footprint_hover">
		<LabelStyle>
			<color>50D2783C</color>
		</LabelStyle>
		<BalloonStyle><bgColor>00000000</bgColor></BalloonStyle>
		<LineStyle>
			<color>FFD2783C</color>
			<width>6</width>
		</LineStyle>
		<PolyStyle>
			<color>50D2783C</color>
			<fill>1</fill>
		</PolyStyle>
	</Style>
	<StyleMap id="footprint_styles">
		<Pair>
			<key>normal</key>
			<styleUrl>#footprint</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#footprint_hover</styleUrl>
		</Pair>
	</StyleMap>